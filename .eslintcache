   </DishImage>\n      )}\n      {isSignatureDish && (\n        <SignatureDish className=\"signature-image\" >\n          <img src={img} alt={alt}></img>\n        </SignatureDish>\n      )}\n      {isPopular && (\n        <PopularRestImage>\n          <img src={img} alt={alt}></img>\n        </PopularRestImage>\n      )}\n      <CardContent className=\"card-content\" price={price} isRest={isRest}> \n        <h2>{title}</h2>\n        <p>{body}</p>\n        {icon && (\n          <Icon className=\"icon\">\n            <img src={icon} alt={alt}></img>\n          </Icon>\n        )}\n        {price && (\n          <Price price={price} className=\"card-price\">\n            <hr></hr>\n          </Price>\n        )}\n      </CardContent>\n    </CardContainer>\n    </>\n  );\n};\n\nexport default Card;\n\nconst CardContainer = styled.div<{readonly isRest?:string}>`\n  width: 360px;\n  overflow: hidden;\n\n  @media only screen and (max-width: 650px) {\n    width:${p => p.isRest ? '160px' : '260px'};\n    overflow: visible;\n  }\n`;\n\nconst CardHeader = styled.div`\nh1{\n  font-size:1.875rem;\n  text-align: center;\n  font-family: HelveticaNeue;\n  font-weight: 100;\n  letter-spacing: 1.77px;\n  font-weight: 500;\n    margin-bottom:23px;\n}\n\n@media only screen and(max-width:650px) {\n  h1{\n    font-size: 0.8rem;\n  }\n}\n`\nconst PopularRestImage = styled.div`\n    height: 224px;\n  img {\n    width: 360px;\n    height: 224px;\n  }\n\n  @media only screen and (max-width: 650px) {\n    img {\n      width: 206px;\n      height: 224px;\n      object-fit: cover;\n    }\n  }\n`;\n\nconst RestImage = styled.div`\n  img {\n    width: 360px;\n  height: 212.2px;\n  object-fit: cover;\n\n  }\n\n  @media only screen and (max-width: 650px) {\n    img {\n      width: 162px;\n  height: 124px;\n      object-fit: cover;\n    }\n  }\n`;\nconst DishImage = styled.div`\n  img {\n    width: 360px;\n  height: 212.2px;\n  object-fit: cover;\ncursor: pointer;\n  }\n\n  @media only screen and (max-width: 650px) {\n    img {\n      width: 157px;\n  height: 100px;\n      object-fit: cover;\n    }\n  }\n`;\nconst SignatureDish = styled.div`\n    height: 290px;\n  img {\n    width: 360px;\n    height: 290px;\n  }\n\n  @media only screen and (max-width: 650px) {\n    img {\n      width: 259.2px;\n      height: 208.8px;\n      object-fit: cover;\n    }\n  }\n`;\n\n\nconst CardContent = styled.div<{readonly price?:number; isRest?:string}>`\n  background-color: #f7e0b2;\n  text-align: center;\n  font-family: HelveticaNeue;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  height:${p => p.price ? '250px' : '100px'};\n  width:${p => p.isRest ? '360px' : 'auto'};\n\n  h2 {\n    font-family: HelveticaNeue;\n    font-weight: 500;  \n    font-size: 2rem;\n    letter-spacing: 2.67px;\n  }\n\n  p {\n    font-size: 1.563rem;\n    font-weight: 100;\n    letter-spacing: 1.67px;\n    width: 80%;\n    margin: 0 auto;\n  }\n\n  @media only screen and (max-width: 650px) {\n    width:${p => p.isRest ? '160px' : 'auto'};\n\n    h2 {\n      font-size:${p => p.isRest ? '1.3rem' : '1.8rem'};\n    }\n\n    p {\n      font-size:${p => p.isRest ? '0.9rem' : '1rem'};\n    }\n  }\n`;\nconst CardContentDish = styled.div<{readonly price?:number; isRest?:string}>`\n  background-color: #f9f4ea;\n  text-align: center;\n  font-family: HelveticaNeue;\n  display: flex;\n  justify-content: space-between;\n  flex-direction: column;\n  height:${p => p.price ? '250px' : '100px'};\n  width:${p => p.isRest ? '360px' : 'auto'};\n\n  h2 {\n    font-family: HelveticaNeue;\n  font-weight: 100;\n    margin-bottom: 2vh;\n    padding-top: 20px;\n    font-size: 2rem;\n    letter-spacing: 2.67px;\n  }\n\n  p {\n    font-size: 1.563;\n    font-weight: 100;\n    letter-spacing: 1.67px;\n    padding-bottom: 20px;\n    width: 80%;\n    margin-top: 2vh;\n    margin: 0 auto;\n    font-size: 1.3rem;\n  }\n\n  @media only screen and (max-width: 650px) {\n    width:${p => p.isRest ? '160px' : 'auto'};\n\n    h2 {\n      font-size:${p => p.isRest ? '1.3rem' : '1.8rem'};\n    }\n\n    p {\n      font-size:${p => p.isRest ? '0.9rem' : '1rem'};\n    }\n  }\n`;\n\nconst Icon = styled.div`\n  img {\n    width: 39px;\n    height: 30px;\n    object-fit: contain;\n    /* margin: 0 auto; */\n  }\n`;\n\nconst Price = styled.div<{ price: number }>`\n  hr {\n    border: none;\n    font-weight: 100;\n    font-family: HelveticaNeue;\n    margin: 0 auto;\n    width: 50%;\n    border-top: 1px solid black;\n    overflow: visible;\n    text-align: center;\n    height: 5px;\n    margin-bottom: 2vh;\n  }\n\n  hr:after {\n    background-color: #f9f4ea;\n    content: \"₪${(p) => p.price}\";\n    font-size: 1.556rem;\n    font-weight: 100;\n    padding: 0 4px;\n    position: relative;\n    top: -13px;\n  }\n  /* width: 100%;\n  font-weight: 100;\n  @media only screen and (max-width: 650px) {\n    font-size: 16px;\n  } */\n`;\n","/Users/nimrodgaver/Desktop/epicure/src/components/PopularRest/PopularRest.tsx",[],"/Users/nimrodgaver/Desktop/epicure/src/components/SignatureDish/SignatureDish.tsx",[],"/Users/nimrodgaver/Desktop/epicure/src/firebase.js",[],"/Users/nimrodgaver/Desktop/epicure/src/pages/Dishes/DishList.tsx",["114"],"import React from \"react\";\nimport { useState, useEffect } from \"react\";\nimport { RouteComponentProps } from \"react-router-dom\";\nimport  Modal  from '../../components/Modal/Modal';\n\n\nimport Card from \"../../components/Cards/Card\";\nimport styled from \"styled-components\";\nimport firebase from \"firebase\";\n\nfunction DishList({ match }: RouteComponentProps<any>) {\n  const [dishes, setDishes] = useState<any[]>([]);\n  const [filteredDishes, setFilteredDishes] = useState<any[]>([]);\n  const [header, setHeader] = useState<any>({});\n  const [selectedDishId, setselectedDishId] = useState(null);\n  const [showModal, setShowModal] = useState<any>(false);\n\n  const openModal = () => {\n    setShowModal((prev:any) => !prev);\n  };\n\n  useEffect(() => {\n    fetchHeader();\n    fetchData();\n  }, []);\n\n  const fetchData = async () => {\n    const db = firebase.firestore();\n    let restaurantsRef = db\n      .collection(\"restaurants\")\n      .doc(`${match.params.id}`)\n      .collection(\"dishes\");\n    let allRestaurants = await restaurantsRef.get();\n    let dataArray: any = [];\n    for (const doc of allRestaurants.docs) {\n      dataArray.push({ ...doc.data(), id: doc.id });\n    }\n    setDishes(dataArray);\n    setFilteredDishes(dataArray)\n  };\n\n  const fetchHeader = async () => {\n    const db = firebase.firestore();\n    var docRef = db.collection(\"restaurants\").doc(`${match.params.id}`);\n    docRef\n      .get()\n      .then(function (doc) {\n        if (doc.exists) {\n          console.log(\"Document data:\", doc.data());\n          setHeader(doc.data());\n        } else {\n          console.log(\"No such document!\");\n        }\n      })\n      .catch(function (error) {\n        console.log(\"Error getting document:\", error);\n      });\n  };\n\n  const filterHandler = (e: any) => {\n    if (e.target.value === \"breakfest\") {\n      setFilteredDishes(dishes.filter((card) => card.breakfest));\n    }\n    if (e.target.value === \"launch\") {\n      setFilteredDishes(dishes.filter((card) => card.launch));\n    }\n    if (e.target.value === \"dinner\") {\n      setFilteredDishes(dishes.filter((card) => card.dinner));\n    }\n  };\n\n  ;\n  \n\n  return (\n    <Container>\n    <DishContainer>\n        <HeaderImage mobileImage={header.phoneImage}>\n          <img src={header.imageHeader} alt={header.alt}></img>\n        </HeaderImage>\n      <HeaderContainer>\n          <HeaderContent>\n            <h1>{header.title}</h1>\n            <p>{header.body}</p>\n          </HeaderContent>\n      </HeaderContainer>\n      <IsOpen isOpen={header.open}> \n            <img src={header.clockImage} alt={header.alt}></img>\n            <h2>\n              <span>Open</span> now\n            </h2>\n      